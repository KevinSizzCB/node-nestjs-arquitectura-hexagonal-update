{
  "name": "node-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "nest build && cross-env NODE_ENV=production node dist/main",
    "lint": "eslint {src,test}/**/*.ts",
    "lint:fix": "eslint {src,test}/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "jest \"test/unitarias/.*\\.spec\\.ts\"",
    "test:e2e": "jest \"test/e2e/.*\\.e2e-spec\\.ts\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "nest build && npm run typeorm -- migration:create -n",
    "migration:generate": "nest build && npm run typeorm -- migration:generate -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:drop": "npm run typeorm -- schema:drop",
    "migration:show": "npm run typeorm -- migration:show"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^8.1.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/swagger": "^5.1.1",
    "@nestjs/typeorm": "^8.0.2",
    "@types/ip": "^1.1.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.2",
    "ip": "^1.1.5",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^7.4.0",
    "sinon": "^11.1.2",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.22",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "27.0.2",
    "@types/node": "^16.11.1",
    "@types/sinon": "^10.0.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "supertest": "^6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>"
    ]
  }
}
